{
  "name": "voud",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "flow": "flow",
    "flow-stop": "flow stop",
    "rn-clean-cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "ios": "react-native run-ios --simulator='iPhone 8 Plus'",
    "ios-device": "react-native run-ios --device ",
    "ios-clean-derived-and-build": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ./ios/build",
    "android": "react-native run-android ",
    "android-release": "react-native run-android --variant=release",
    "android-bundle": "cd android && ./gradlew bundle && cd ..",
    "android-build": "cd android && ./gradlew assembleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "android-bundle-js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
